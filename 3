#include <stdio.h>  
int is_leap_year(int);  
  
int main()
{  
    int year;  
    printf("please input year:");  
    scanf("%d", &year);  
	if (year % 4  == 0 && year % 100 != 0  || year % 400 == 0)  
        printf("%d is leap year\n", year);  
    else  
        printf ("%d not leap year\n", year); 

	return 0;
}  
